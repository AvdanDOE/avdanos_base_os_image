#!/bin/zsh
# Placeholder for now

# Initialize session sentinel
if [[ -f /tmp/avdanos/doebox-session]]; then
    cp -r /etc/avdanos/dock /tmp/avdanos
    chmod $USER:$USER /tmp/avdanos/dock
    # Start EWW
    eww -c /tmp/avdanos/dock daemon
    eww -c /tmp/avdanos/dock open-many dockdynamic dockmain dockstatic dockstatus
else
    echo "Unable to start dock. Are you running in DOEBox?"
fi